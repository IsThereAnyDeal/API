{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Price",
  "type": "object",
  "properties": {
    "amount": {
      "type": "number",
      "minimum": 0
    },
    "amountInt":{
      "type": "integer",
      "minimum": 0
    },
    "currency": {
      "type": "string",
      "pattern": "[A-Z]{3}"
    }
  },
  "required": ["amount", "amountInt", "currency"]
}
